{
    "name": "dostman",
    "version": "1.0.0",
    "description": "Maintain your API development, documentation and experimentation all within your codebase.",
    "main": "src/main/index.js",
    "bin": {
        "dostman": "src/index.js"
    },
    "files": [
        "src/*"
    ],
    "scripts": {
        "dev": "nodemon src/index.js",
        "start": "node src/index.js",
        "test": "jest",
        "prettify": "npx prettier --write .",
        "prettify-output": "npx prettier --write ./output.json",
        "remove-output-files": "rm ./sample-requests/*.json || true"
    },
    "keywords": [
        "Postman",
        "REST",
        "API",
        "Client"
    ],
    "author": "Ege Cavusoglu egecavusoglu99@gmail.com https://egecavusoglu.com",
    "license": "MIT",
    "devDependencies": {
        "jest": "^27.0.6",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.1"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "chalk": "^4.1.1",
        "glob": "^7.1.7"
    }
}
